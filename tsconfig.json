{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "jsx": "react",
    "jsxFactory": "h",
    "target": "es5",
    "skipLibCheck": true
  },
  
  "exclude": ["node_modules"]
}
